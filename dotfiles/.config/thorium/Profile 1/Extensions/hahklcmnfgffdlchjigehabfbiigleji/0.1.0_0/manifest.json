{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "commands": {
      "launch": {
         "description": "Play with MPV",
         "suggested_key": {
            "default": "Ctrl+Space"
         }
      }
   },
   "description": "Sends videos in pages like youtube to an external MPV player",
   "homepage_url": "https://github.com/thann/play-with-mpv",
   "icons": {
      "128": "mpv-logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Play with MPV",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "page_action": {
      "default_icon": "mpv-logo.png",
      "default_title": "Play with MPV"
   },
   "permissions": [ "storage", "activeTab", "contextMenus", "http://localhost/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.0"
}
